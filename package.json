{
  "name": "webpack-ts-scss",
  "version": "0.1.0",
  "description": "Webpack ts-scss boilerplate",
  "main": "src/index.ts",
  "author": "Sam-Shpakov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sam-Shpakov/webpack-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sam-Shpakov/webpack-boilerplate.git"
  },
  "scripts": {
    "dev": "webpack serve --open --color --hot --mode=development",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -f -r build",
    "analyze": "cross-env analyze=true NODE_ENV=production webpack",
    "prettier:format": "prettier --write \"**/*.+(ts|tsx|scss)\"",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:fix-staged": "eslint --fix",
    "fix": "run-s prettier:format lint:fix",
    "fix-staged": "lint-staged",
    "svgo": "cross-env svgo --pretty --indent=2 --disable=removeViewBox,mergePaths --enable=inlineStyles  --config svgo-config.json $FILE",
    "svgo-all": "cross-env svgo -r -f src --pretty --indent=2 --disable=removeViewBox,mergePaths --enable=inlineStyles  --config svgo-config.json"
  },
  "browserslist": [
    "last 2 versions",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.12.10",
    "@types/classnames": "^2.3.0",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/html-webpack-plugin": "^3.2.5",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/node": "^15.0.2",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.0",
    "@types/webpack-dev-server": "^3.11.4",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4.3.7",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.14",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "svgo": "^2.3.0",
    "svgo-loader": "^3.0.0",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "typescript-plugin-css-modules": "^3.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.36.2",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
